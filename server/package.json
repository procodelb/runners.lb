{
  "name": "soufian-erp-server",
  "version": "1.0.0",
  "description": "Backend server for Soufian ERP System",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "init:admin": "node scripts/initAdmin.js",
    "setup:db": "node scripts/setupDatabase.js",
    "init:sqlite": "node scripts/initSQLite.js",
    "init:supabase": "node scripts/initSupabase.js",
    "init:neon": "node scripts/initNeon.js",
    "setup:neon": "node scripts/ensureNeonSetup.js",
    "migrate:neon:dev": "node scripts/neonDevMigrate.js",
    "migrate:to-postgres": "node scripts/migrateToPostgreSQL.js",
    "migrate:latest": "node -e \"const {Pool}=require('pg');const fs=require('fs');const pool=new Pool({connectionString:process.env.DATABASE_URL});pool.query(fs.readFileSync('../migrations/001_initial_schema.sql','utf8')).then(()=>{console.log('Migration complete');process.exit(0)}).catch((e)=>{console.error('Migration failed:',e);process.exit(1)});\"",
    "seed:run": "node -e \"const {Pool}=require('pg');const fs=require('fs');const pool=new Pool({connectionString:process.env.DATABASE_URL});pool.query(fs.readFileSync('../seeds/001_initial_seed.sql','utf8')).then(()=>{console.log('Seed complete');process.exit(0)}).catch((e)=>{console.error('Seed failed:',e);process.exit(1)});\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.12.0",
    "puppeteer": "^24.16.0",
    "socket.io": "^4.4.7",
    "soufian-erp-system": "file:..",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
